.open
{
    background: url(../Images/cart_images/openCloseArrows.png) no-repeat scroll 0 0;
    cursor: pointer;
    display: inline-block;
    margin-top: 15px;
    right: 15px;
    height: 22px;
    width: 22px;
    float: right;
}

*
{
    margin: 0px;
    padding: 0px;
}
body
{
    font-family: Arial, Helvetica, sans-serif;
    line-height: 2em;
    text-align: left;
}
.main_container
{
    width: 950px;
    margin: 0 auto;
}
.topbar
{
    height: 90px;
    background-color: #0a487a;
    margin-bottom: 70px;
}
.topbar img
{
    padding-top: 10px;
}
.mainContent
{
    background: #fff none repeat scroll 0 0;
    clear: both;
    margin: 36px auto 48px;
    position: relative;
    width: 960px;
}
.cart_head h2
{
    color: #205081;
    font-family: CircularPro-Book, Helvetica, Arial, sans-serif;
    font-size: 19px;
    line-height: 24px;
    margin-bottom: 5px;
}
hr
{
    background: #0a487a none repeat scroll 0 0;
    border: medium none;
    height: 1px;
    margin: 0;
}
.cartleftcontainer
{
    width: 770px;
    padding: 0px 0px;
    box-sizing: border-box;
    float: left;
}
.cartrightcontainer
{
    width: 180px;
    padding: 5px 0px 10px;
    padding-left: 10px;
    box-sizing: border-box;
    text-align: center;
    height: 100%;
    float: right;
    line-height: 21px;
}

.card_aded_list
{
    display: table;
    width: 100%;
}
.card_aded_list .cart_item
{
    display: table-row;
    width: 100%;
}
.cart_item .item_title
{
    padding-left: 0px !important;
}
.card_aded_list .cart_item div
{
    display: table-cell;
    padding: 10px 6px;
    box-sizing: border-box;
    vertical-align: middle;
}
.card_aded_list .cart_item div a
{
   /* width: 30px;*/
    border-radius: 50%;
    height: 30px;
    display: block;
}
.card_aded_list .cart_item div
{
    border-bottom: 1px solid #b7b7b7;
}
.card_aded_list .cart_item .item_price
{
    font-size: 18px;
    color: #559e35;
}
.card_aded_list .cart_item div a:hover
{
}
.item_confirm
{
    width: 99px;
}
.m-b-50
{
    margin-bottom: 50px;
}
.secondhead
{
    font-size: 18px;
    text-transform: uppercase;
    font-weight: normal;
    margin: 7px 0px;
    color: #205081;
}

.products_list
{
    display: table;
    width: 100%;
    padding-bottom: 5px;
}
.products_list .product_item .item_title
{
    padding-left: 0px !important;
}
.products_list .product_item
{
    display: table-row;
    width: 100%;
    border: 1px solid #454545;
}
.products_list .product_item div
{
    display: table-cell;
    padding: 10px 6px;
    box-sizing: border-box;
    vertical-align: middle;
}
.products_list .product_item .product_title
{
    padding-left: 0px !important;
}
.products_list .product_item div a
{
    width: 30px;
    border-radius: 50%;
    height: 30px;
    display: block;
}
.products_list .product_item div
{
    border-bottom: 1px solid #b7b7b7;
    border-top: 1px solid #b7b7b7;
}
.products_list .product_item div:first-child
{
    border-left: 1px solid #b7b7b7;
}
.products_list .product_item div:last-child
{
    border-right: 1px solid #b7b7b7;
}
.products_list .product_item .product_price
{
    font-size: 20px;
    color: #559e35;
}
.products_list .product_item div a:hover
{
}

