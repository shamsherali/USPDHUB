.button.buttonSm .buttonTxt
{
    border-radius: 6px;
    padding: 0 8px;
}

.button .buttonTxt
{
    color: #fff;
    display: block;
    padding: 0 14px;
    position: relative;
}

.button
{
    background: #67ab49;
    border: none;
    border-radius: 3px;
    cursor: pointer;
    display: inline-block;
    font-size: 15px;
    line-height: 15px;
    padding: 2px;
    text-decoration: none;
    text-align: center;
    font-family: "Helvetica Neue" ,Helvetica,Arial,sans-serif;
    font-weight: 300;
    letter-spacing: 1px;
    position: relative;
    top: -2px;
    margin-top: 2px;
    min-width: 60px; /* margin-left: 18px; */
    height: 20px;
}

#ddlquqntity option
{
    background-color: white;
}

.close
{
    background-position: 0 -22px;
}

.close:hover
{
    background-position: 0 -44px;
}
.clear
{
    clear: both;
    padding-bottom: 20px;
}
.product_description
{
    padding: 0px !important;
    width: 150px;
}
.dvddl
{
    height: 30px;
    width: 80px !important;
    float: left;
    padding-top: 7px !important;
    text-align: center;
}
.select-menu-quantity
{
    border: 0 !important;
    display: table-cell;
}
.select-menu-title
{
    margin-left: 10px;
    border: 0 !important;
    display: table-cell;
}
.m-b-10
{
    margin-bottom: 10px;
}
.nocart.emptyCart
{
    background: url(../images/dialogInfoNew.png) no-repeat scroll 10px 12px;
    background-size: 16px;
    color: #707070;
}
.nocart
{
    background: #e8f4ff url(../images/dialogInfo.png) no-repeat scroll 10px 10px;
    border: 1px solid #205081;
    border-radius: 6px;
    color: #367fbf;
    margin-bottom: 20px;
    margin-top: 20px;
    padding: 10px 30px 10px 38px;
    position: relative;
}
.fieldset
{
    border: 1px solid #0a487a;
    border-radius: 6px;
    height: auto;
    padding-bottom: 20px;
}
.subprice
{
    color: #FF4C3B !important;
}
.sideColumn
{
    width: 180px;
    float: right;
    position: relative;
    padding-top: 20px;
}
.couponcode
{
    width: 100px;
}
.couponcode:hover .coupontooltip
{
    display: inline-block;
}
.coupontooltip
{
    font-weight: normal;
    font-size: 14px;
    display: none;
    background: #D9E8FF;
    margin-left: 10px;
    margin-bottom: 100px;
    border: 1px dashed #297CCF;
    padding: 10px;
    position: absolute;
    z-index: 1000;
    width: 320px;
    height: 100px;
    color: Black;
}
.couponcode:hover .questiontooltip
{
    display: inline-block;
}
.questiontooltip
{
    font-weight: normal;
    font-size: 14px;
    display: none;
    background: #D9E8FF;
    margin-left: 10px;
    margin-bottom: 100px;
    border: 1px dashed #297CCF;
    padding: 10px;
    position: absolute;
    z-index: 1000;
    width: 320px;
    height: 30px;
    color: Black;
}
.couponcode1
{
    position: absolute;
    display: inline-block;
    width: 25px;
    float: left;
    margin-left: 5px;
    margin-top: 3px;
}
.couponcode1:hover .questiontooltip1
{
    visibility: visible;
}
.couponcode1 .questiontooltip1
{
    visibility: hidden;
    width: 320px;
    background-color: #D9E8FF;
    color: Black;
    padding: 10px;
    min-height: 30px;
    border: 1px dashed #297CCF;
    font-weight: normal;
    font-size: 14px;
    margin-bottom: 100px; /* Position the tooltip */
    position: absolute;
    z-index: 1000;
    top: -5px;
    left: 75%;
}
.btnBackPricing {
  background: #3498db;
  background-image: -webkit-linear-gradient(top, #3498db, #2980b9);
  background-image: -moz-linear-gradient(top, #3498db, #2980b9);
  background-image: -ms-linear-gradient(top, #3498db, #2980b9);
  background-image: -o-linear-gradient(top, #3498db, #2980b9);
  background-image: linear-gradient(to bottom, #3498db, #2980b9);
  -webkit-border-radius: 10;
  -moz-border-radius: 10;
  border-radius: 10px;
  font-family: Arial;
  color: #ffffff;
  font-size: 14px;
  padding: 10px 10px 10px 10px;
  text-decoration: none;
}

.btnBackPricing:hover {
  background: #3cb0fd;
  background-image: -webkit-linear-gradient(top, #3cb0fd, #3498db);
  background-image: -moz-linear-gradient(top, #3cb0fd, #3498db);
  background-image: -ms-linear-gradient(top, #3cb0fd, #3498db);
  background-image: -o-linear-gradient(top, #3cb0fd, #3498db);
  background-image: linear-gradient(to bottom, #3cb0fd, #3498db);
  text-decoration: none;
}
 .pull-top {
    font-size: 14px !important;
    font-weight: bold;
    text-align: center;
}
.pull-top a {
    color: #E45641;
}