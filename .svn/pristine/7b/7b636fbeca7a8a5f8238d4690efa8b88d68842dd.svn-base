.addcart .nextbutton
{
    padding: 10px 20px;
    background-color: #dd8e11;
    color: #fff;
    text-decoration: none;
}
.addcart .nextbutton img
{
    width: 22px;
    position: relative;
    top: 4px;
}
.addcart h4
{
    color: #559e35;
    margin: 10px;
    font-size: 22px;
    font-weight: normal;
}
.addcart p
{
    font-size: 12px;
    color: #000;
}
.m-b-30
{
    margin-bottom: 30px;
}
.product_confirm
{
    width: 80px;
}
.product_title
{
    text-align: left;
}

.btn
{
    -webkit-border-radius: 10px;
    -moz-border-radius: 10px;
    border-radius: 10px;
    font-family: Arial;
    color: #ffffff;
    font-size: 15px;
    background: #67ab49;
    padding: 10px 10px 10px 10px;
    text-decoration: none;
    border: 0px;
}

.btn:hover
{
    background: #618751;
    text-decoration: none;
}

.product_img
{
    width: 50px;
}

.item_img
{
    width: 50px;
}

.item_title
{
    width: 275px;
}

.product_title
{
    width: 275px;
}

.item_price
{
    width: 100px;
}

.product_price
{
    width: 100px;
}

