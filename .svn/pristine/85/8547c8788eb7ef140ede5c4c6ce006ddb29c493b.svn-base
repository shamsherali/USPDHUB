<%@ Page Language="C#" AutoEventWireup="true" Inherits="showimag" CodeBehind="showimag.aspx.cs" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title>View Image</title>
</head>
<body>
    <form id="form1" runat="server">
    <div>
        <table cellpadding="0" cellspacing="0" border="0" width="100%">
            <tr>
                <td>
                    <img src='<%=ImageName %>' border="0" />
                </td>
            </tr>
            <tr>
                <td colspan="2" style="color: Red;">
                    We are unable to upload your image; the image is too small and will appear distorted
                    as in the sample above. The minimum size for a photo is 320 X 240. Please select
                    another photo and try again.
                </td>
            </tr>
        </table>
    </div>
    </form>
</body>
</html>
