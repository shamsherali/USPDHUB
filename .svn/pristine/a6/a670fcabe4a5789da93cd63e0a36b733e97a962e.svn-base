.scrolloption
{
    background-color: #E0E0E0;
    border-radius: 5px;
    height: 110px;
    padding: 10px;
    position: relative;
}
.scrolloption:after
{
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
