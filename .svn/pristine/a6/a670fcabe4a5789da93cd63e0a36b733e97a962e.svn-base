}
.Clearfix:after
{
    clear: both;
    content: ".";
    display: block;
    height: 0;
    line-height: 0;
    visibility: hidden;
}
.Clear
{
    clear: both;
}
.Clearfix
{
    display: inline-block;
}
.Bdr
{
    border: medium none !important;
}
