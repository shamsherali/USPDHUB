@media screen and (max-width:667px)
{

    *
    {
        margin: 0;
        padding: 0;
    }

    #outerwrapper
    {
        width: 100%;
        height: auto;
        overflow: hidden;
    }

    #wrapper
    {
        width: 100%;
        height: auto;
    }

    #banner
    {
        width: 100%;
        height: auto;
    }

    #bannerright
    {
        width: 100%;
        height: auto;
    }

    .pricingplan-page h4
    {
        font-size: 110%;
        letter-spacing: 1px;
        text-align: center;
    }

    .plans, .plan
    {
        width: 100%;
        height: auto;
        zoom: 1;
    }

    .plan
    {
        width: 80%;
        height: auto;
        margin-left: 10%;
        margin-right: 10%;
        margin-bottom: 5%;
    }

}






