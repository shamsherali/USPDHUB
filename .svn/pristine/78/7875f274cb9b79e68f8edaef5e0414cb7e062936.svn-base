        height: auto;
        margin-left: 25%;
        margin-right: 25%;
    }

    #bannerright
    {
        width: 100%;
        height: auto;
    }

    #social
    {
        text-align: center;
        width: 100%;
        height: auto;
    }

    #signup
    {
        width: 100%;
        height: auto;
    }
    #signup2
    {
        width: 30%;
        height: auto;
        margin-left: 35%;
        margin-right: 35%;
    }

    #logictree
    {
        text-align: center;
        width: 100%;
        height: auto;
        margin-bottom: 5%;
    }

    #contentleft620
    {
        width: 100%;
        height: auto;
    }

    #col619
    {
        width: 90%;
        margin-left: 5%;
        margin-right: 5%;
    }

    #col619 h1
    {
        font-size: 150%;
        text-align: center;
    }

    #col405l
    {
        width: 90%;
        height: auto;
        margin-left: 5%;
        margin-right: 5%;
    }

    #col405l
    {
        text-align: justify;
    }

    #Div1
    {
        width: 100%;
        height: auto;
    }

    #Div1 img
    {
        width: 100%;
        height: auto;
    }

    #Div1 h2, #Div1 p
    {
        text-align: center;
    }

    #Div1 p
    {
        width: 80%;
        height: auto;
        margin-left: 10%;
        margin-right: 10%;
    }


    .not-heading
    {
        width: 60%;
        text-align: center;
        margin-left: 20%;
        margin-right: 20%;
    }
    .not1
    {
        width: 20%;
    }

    #col185r
    {
        text-align: center;
        width: 100%;
    }

    #slideshow
    {
        width: 60%;
        height: auto;
        margin-top: 10%;
        margin-left: 20%;
        margin-right: 20%;
    }


    #footer
    {
        width: 100%;
        height: auto;
        padding-top: 50px;
    }

    #footer p
    {
        width: 100%;
        height: auto;
        text-align: center;
    }
    #navigation
    {
        width: 100%;
        height: auto;
    }

    ul#nav
    {
        width: 100%;
        height: auto;
        margin: 0;
        padding: 0;
        list-style-type: none;
        display: inline;
        text-align: center;
    }

    ul#nav li
    {
        width: 100%;
        height: auto;
        margin: 0;
        padding: 0;
        color: #065baa;
        text-align: center;
    }
    #col550fr
    {
        width: 90%;
        margin-left: 5%;
        margin-right: 5%;
    }

    #col550fr h1
    {
        font-size: 150%;
        text-align: center;
    }
    #contactform
    {
        width: 100%;
        padding: 0px;
        margin-left: 0px;
    }
    #login
    {
        width: 100%;
        height: 100px;
        background: none;
        border: 0;
    }

    #login h1
    {
        height: 100px;
    }
    #tbllogin
    {
        padding: 35px;
        width: 100%;
    }

}


