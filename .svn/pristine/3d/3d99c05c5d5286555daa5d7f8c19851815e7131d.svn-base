    background-image: url(../images/VideoImages/twovie_play.png);
}

#col285fr
{
    text-align: center;
    width: 300px;
    float: left;
    margin: 65px 0px 0px 43px;
}
#col285fl
{
    text-align: center;
    width: 285px;
    float: left;
}
#col520fl
{
    width: 520px;
    float: left;
}
#contentright500
{
    width: 500px;
    float: right;
}
#col430fl
{
    width: 430px;
    float: left;
}
#innerwrapper
{
    text-align: left;
    width: 900px;
    margin-right: auto;
    margin-left: auto;
}
.publishtext
{
    color: #3c3c3c;
    font-family: Verdana, Geneva, sans-serif;
    font-size: 14px;
}
.basicalign
{
    font-size: 20px;
    font-weight: bold;
}
.plan-features
{
    margin-bottom: 15px;
    line-height: 2;
    font-size: 16px;
    color: #999;
    text-align: center;
    padding-left: 20px;
}
.plan-features li
{
    list-style-type: none;
    text-align: left;
    font-family: Verdana, Geneva, sans-serif;
    position: relative;
    color: #181819;
    font-size: 14px;
}
.plan-features li:before
{
    content: "✔";
    position: absolute;
    top: 0px;
    left: -13px;
}

/* -----------------------------------------*/
/* Media Screen for Mobile Devices @ 667px */
/* -----------------------------------------*/

@media screen and (max-width:667px)
{

    *
    {
        margin: 0;
        padding: 0;
    }

    #outerwrapper
    {
        width: 100%;
        height: auto;
        overflow: hidden;
    }

    #innerwrapper
    {
        width: 100%;
        height: auto;
        overflow: hidden;
    }

    #wrapper
    {
        width: 100%;
        height: auto;
    }

    #banner
    {
        width: 100%;
        height: auto;
    }

    #bannerright
    {
        width: 100%;
        height: auto;
    }

    #brand
    {
        width: 100%;
        height: auto;
    }

    #brand img
    {
        max-width: 100%;
        width: 60%;
        height: auto;
        margin-right: 20%;
        margin-left: 20%;
    }


    #social
    {
        text-align: center;
        width: 100%;
        height: auto;
    }

    #navspace
    {
        width: 100%;
        height: auto;
    }

    ul#nav
    {
        width: 100%;
        height: auto;
        list-style-type: none;
        display: inline-block;
        text-align: center;
    }

    ul#nav li
    {
        width: 100%;
        height: auto;
        margin: 0;
        padding: 0;
        text-align: center;
    }

    ul#nav a
    {
        color: #b8b3b6;
        font-size: 100%;
        line-height: 30px;
        text-decoration: none;
        text-align: center;
        width: 100%;
        height: auto;
        display: inline-block;
    }


    #container
    {
        width: 100%;
        text-align: center;
        margin: 0;
        padding: 0;
    }

    /* #container p{
    width: 90%;
    text-align: center;
    font-size: 140%;
    margin:5%;
    line-height: 30px;
    padding: 0;
    } */

    .ownapp
    {
        font-size: 140%;
        line-height: 30px;
        margin: 5;
        color: #db5634;
    }


    #contentleft564
    {
        width: 100%;
        height: auto;
        text-align: center;
    }


    .extendway
    {
        width: 90%;
        font-size: 125%;
        text-align: center;
        line-height: 30px;
        text-transform: uppercase;
        margin: 2% 5%;
    }

    #contentleft564
    {
        width: 90%;
        font-size: 90%;
        text-align: center;
        line-height: 20px;
        text-transform: uppercase;
        margin: 0 5%;
    }


    #contentleft564 b
    {
        width: 90%;
        font-size: 90%;
        text-align: center;
        line-height: 20px;
        text-transform: uppercase;
        margin: 0 5%;
    }


    .clear10
    {
        width: 100%;
        height: auto;
        margin-top: 15%;
        margin-bottom: 15%;
    }


    .plan-features li
    {
        color: #181819;
        font-size: 80%;
    }

    .not-heading
    {
        width: 90%;
        text-align: center;
        margin-left: 5%;
        margin-right: 5%;
    }

    .not1
    {
        width: 25%;
    }

    #contentright263
    {
        width: 100%;
        height: auto;
    }


    .pricingplan-page
    {
        width: 100%;
        height: auto;
    }


}

