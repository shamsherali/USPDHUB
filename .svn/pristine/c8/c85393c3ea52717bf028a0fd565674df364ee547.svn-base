@import url(http://fonts.googleapis.com/css?family=Open+Sans:400,400italic,600,600italic,700,700italic,300,300italic);
@import url(http://fonts.googleapis.com/css?family=Roboto+Condensed:700,400,300);
@import url(http://fonts.googleapis.com/css?family=Open+Sans+Condensed:700,300);
/*
font-family: 'Open Sans', sans-serif;
font-family: 'Roboto Condensed', sans-serif;
font-family: 'Open Sans Condensed', sans-serif;
*/
html, body
{
    color: #282828;
    font-family: 'Open Sans' , sans-serif;
    line-height: 1.7;
    font-size: 14px;
    overflow-x: hidden;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    -ms-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    transition: all 0.5s ease;
    background-color: #f1f1f1;
    height: 100%;
}
/* default codes */
h1, h2, h3, h4, h5, h6
{
    color: #282828;
    margin-bottom: 20px;
    font-family: 'Open Sans' , sans-serif;
    font-weight: 400;
}
h1
{
    font-size: 20px;
    font-weight: 600;
    text-transform: uppercase;
    text-shadow: 0 0 1px rgba(0,0,0,0.5);
    color: #1c97cb;
    border-bottom: 1px dashed #454545;
    padding-bottom: 10px;
}
h4
{
    font-size: 17px;
    font-weight: 600;
    text-transform: uppercase;
    --text-shadow: 0 0 1px rgba(0,0,0,0.5);
    color: #1c97cb;
    margin: 0px 0px 10px 0px;
}
p
{
    font-family: 'Open Sans' , sans-serif; /*text-shadow: 0 0 1px rgba(0,0,0,0.4);*/
    font-size: 14px;
}
li
{
    list-style-type: none;
}
.nopadding
{
    padding: 0px !important;
}
a
{
    color: #333;
    text-decoration: none;
}
a:hover
{
    text-decoration: none;
}
#wrapper
{
    min-height: 100%;
    position: relative;
}
#header
{
    background-color: #fff;
}
.headercontent
{
    padding: 0px 0px 0px 0px;
}
.headercontent p:first-child
{
    color: #888;
}
.headercontent h4 a
{
    color: #3cbc9f;
    font-size: 14px;
    text-decoration: underline;
    text-transform: capitalize;
}
.headermenu li
{
    padding: 0px 30px !important;
}
.headermenu li:last-child
{
    padding: 0px 0px 0px 30px !important;
}
.headermenu li a:before
{
    content: "";
    width: 13px;
    height: 13px;
    border-radius: 50%;
    background-color: #3cbc9f;
    display: block;
    position: absolute;
    margin-left: -20px;
    margin-top: 7px;
}
.headermenu li a
{
    color: #000;
    text-shadow: 0 0 1px rgba(0,0,0,0.6);
    font-size: 15px;
    padding: 0px 0px 10px 0px;
}
#body
{
    padding-bottom: 60px; /*min-height:854px;*/
}
.timebar
{
    background-color: #3cbc9f;
}
.timebarlist
{
    color: #fff;
    text-align: right;
    margin: 6px 0px;
}
.timebarlist li
{
    line-height: 2.5em;
}
.dropdown a#dLabel
{
    color: #fff;
    margin-left: 20px;
}
.dropdown a#dLabel:hover
{
    text-decoration: none;
}
.timebar .caret
{
    border-left: 9px solid transparent;
    border-right: 9px solid transparent;
    border-top: 10px dashed;
    display: inline-block;
    height: 0;
    margin-left: 2px;
    vertical-align: middle;
}
.accountnotif
{
    background-color: #fff;
    margin-top: 10px;
    margin-bottom: 10px;
}
.accountnotif h3
{
    margin: 0px;
    display: inline-block;
    font-weight: 600;
    font-size: 22px;
    color: #555;
}
.accountnotif header
{
    background-image: url(../images/liteimages/acnot_bg.png);
    background-repeat: repeat-x;
    padding: 10px 20px;
}
.accountnotif header img
{
    float: right;
}
.accountnotifcontent
{
    padding-top: 10px;
}
.accountnotifcontent p
{
    background-color: #f1f1f1;
    padding: 10px 15px;
}
.accountnotifcontent p img
{
    width: 30px;
    margin-right: 15px;
}
.accountnotifcontent p a
{
    color: red;
}
.dashtabs
{
    background-color: #fff;
}
.contacts
{
    background-color: #faaf42;
}
.callbtns
{
    background-color: #0491b6;
}
.manageinvitations
{
    background-color: #3bbb9e;
}
.managenotifications
{
    background-color: #df577d;
}
.dashbox
{
    text-align: center;
    color: #fff;
    padding: 10px 0 0 0;
    margin: 10px 0px;
    height: 260px;
}
.dashbox h4
{
    text-transform: capitalize;
    font-weight: 600;
    font-size: 23px;
    color: #fff;
}
.dashbox p
{
    font-size: 14px;
    height: 45px;
}
.dashbox img
{
    margin-bottom: 20px;
}
.dashbox .row .col-lg-4:first-child img
{
    --position: relative;
    top: 8px;
    padding-right: 5PX;
    margin: 0px 0px 5px 5px;
}
.dashbox .row .col-lg-4:nth-child(2) img
{
    --position: relative;
    top: 8px;
    padding-right: 5PX;
}
.dashbox .row .col-lg-4:nth-child(2)
{
    position: relative;
    top: 7px;
}
.dashbox .row a
{
    color: #fff;
}
.dashbox .row a:hover
{
    color: #f0f0f0;
}
.dashbox a.setupbtn
{
    background: url(../images/liteimages/setup_btn.png) no-repeat left top;
    width: 150px;
    height: 50px;
    display: inline;
    padding: 10px 38px;
    color: #000;
    font-size: 15px;
    margin: 0px 5px 0px 0px;
    position: relative;
    top: 3px;
}
.dashbox a.setupbtn:hover
{
    color: #454545;
}
.dashbox .row img
{
    margin: 0px;
}
.dashboxrow
{
    margin: 0px -10px !important;
}
.clearfix
{
    height: 115px;
}
#footer
{
    position: absolute;
    bottom: 0;
    width: 100%;
    background-color: #2d3e50;
}
.footer li a
{
    color: #3bbb9e;
    line-height: 3.5em;
    padding: 0px 20px;
}
.footer ul
{
    margin: 0px;
}
#timebarlist > .timebardate, .timebartime
{
    display: inline-block;
}
.slidedown
{
    width: 221px;
    height: 126px;
    background-color: #fff;
    -webkit-background-clip: padding-box;
    background-clip: padding-box;
    border: 1px solid #ccc;
    border: 1px solid rgba(0, 0, 0, .15);
    border-radius: 4px;
    -webkit-box-shadow: 0 6px 12px rgba(0, 0, 0, .175);
    box-shadow: 0 6px 12px rgba(0, 0, 0, .175);
    position: absolute;
    margin-left: -95px; /* top: 220px;*/
    display: none;
    padding: 10px 0px 10px 0px;
    color: #FFFFFF;
    z-index: 10001;
}
.slidedown span
{
    font-weight: bold;
    color: #000;
}
.slidedown input
{
    font-weight: bold;
}
.dropdown
{
    padding: 3px;
    margin: 8px 0px;
    font-size: 12px;
    border: 1px solid #0491b6;
    color: #000;
}
#imgSlideZones
{
    cursor: pointer;
    margin-top: -2px;
}
.slidedownsubmit
{
    background-color: #4CAF50;
    border: none;
    color: #FFFFFF;
    padding: 2px 10px;
    text-align: center;
    text-decoration: none;
    font-size: 14px;
    border-radius: 3px;
    cursor: pointer;
}
