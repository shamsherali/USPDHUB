*
{
    margin: 0;
}
#content
{
    width: 980px;
    margin: 0 auto;
    z-index: 99999;
    background: url(../images/ISHOuterImages/bg_banner.png) repeat-y;
    padding: 0px 10px 10px 10px;
    overflow: hidden;
    position: relative;
    font-family: Tahoma, Geneva, sans-serif;
    font-size: 15px;
    line-height: 22px;
}
#aboutcontent
{
    width: 560px;
    float: left;
}
#abtright
{
    width: 390px;
    float: left;
    margin-left: 10px;
}
#abtright img
{
    margin-top: 20px;
}
.listscl
{
    list-style-image: url(../../images/ISAOuterImages/list_bullet.png);
    margin-left: 30px;
    padding-left: 0px;
    margin-bottom: 20px;
}
.listscl i
{
    color: #f15323;
    margin-right: 10px;
    font-size: 17px;
}
#content h4
{
    color: #1c97cb;
    margin-top: 25px;
    margin-bottom: 5px;
}
.listbttom
{
    bottom: 0px;
    position: absolute;
}
.padding40
{
    padding-top: 40px;
}
.not1
{
    float: left;
    margin-right: 0px;
    text-align: center;
    margin: 20px;
}
.contentblock
{
    text-align: center;
}
.contentblock h2
{
    font-family: Arial, Helvetica, sans-serif;
    font-size: 25px;
    font-weight: bold;
    padding: 0px;
    line-height: normal;
    color: #2a2a2a;
}
.price
{
    font-size: 20px;
    color: #444444;
    font-weight: bold;
    text-align: center;
    padding: 10px;
    font-family: Arial, Helvetica, sans-serif;
    display: inline-block;
    width: 430px;
}
#price a:link, a:visited, a:hover
{
    /*color: #FFF;*/
    width: 120px;
}
#price h2
{
    line-height:18px;
    margin-top:10px;
}
.left {
    text-align: left;
}
#login {
    width: 490px;
    padding: 0px 10px;
    z-index: 99999;
    top: 55px;
    margin: 0 auto;
    background: url(../images/Homepage/loginbg-mid.gif) repeat-y;
    border: #ECECEC solid 1px;
}
.left .header
{
    padding-bottom:8px;
    display:inline-block;
}
.formwrap {
    width: 100%;
    float: left;
}
#login h2 {
    margin: 0px 0px 30px 0px;
}
.label1 {
    font-size: 14px;
    color: #000;
    text-align: left;
    float: left;
    padding: 4px 0px 0px 30px;
