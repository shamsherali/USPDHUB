    margin: 10px auto;
}
.txtfild1 {
    width: 265px;
    height: 30px;
    border: 1px solid #f2f5f5;
    padding: 2px;
    float: left;
    outline: none;
    background: url(../images/Homepage/bg_txtfild.jpg) no-repeat;
}
.clear15 {
    clear: both;
    height: 15px;
}
#divremember {
    padding-left: 20px;
}
.label2 {
    font-size: 13px;
    color: #000;
    text-align: left;
    float: left;
    padding: 4px 0px 0px 0px;
    width: 280px;
    font-family: Arial, Helvetica, sans-serif;
}
.submit1 {
    margin: 0 auto;
    text-align: center;
}
.loginheader h1 {
    color: #ff4e00;
    font-size: 33px;
    font-family: "Times New Roman", Georgia, Times, serif;
    font-weight: bold;
    line-height: 36px;
    margin: 0 0 12px;
}
#loginbox .checkbox
{
    left: -20px;
}
#divremember
{
    padding-left: 20px;
}
#forgot a
{
    color: #0d9cd8;
}
#signupcontent
{
    width: 536px;
    margin: 0 auto;
    z-index: 99999;
    padding: 0px 10px 10px 10px;
