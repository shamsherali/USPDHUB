/**** SLIDER ***/
.viewer
{
    height: 385px;
    width: 580px;
    overflow: hidden;
    position: relative;
}
.slideimages
{
    position: absolute;
    top: 0;
    left: 0;
}
.slideimages img
{
    float: left;
    margin-top: 10px;
}
#slides {
    left: 4px;
    top: 18px;
    z-index: 100;
}
.slides_container {
    display: none;
    overflow: hidden;
    width: 570px;
}
.slides_container a {
    display: block;
    height: 400px;
    width: 570px;
}
.slides_container a img {
    display: block;
}
/* 	Pagination */

.pagination {
	margin:-23px auto 0;
	width:120px;
	float:right;
	margin-right:35px;
}

.pagination li {
	float:left;
	margin:0 1px;
	list-style:none;
}

.pagination li a {
	display:block;
	width:12px;
	height:0;
	padding-top:12px;
	background-image:url(../images/OuterImages/pagination.png);
	margin-left:5px;
	background-position:0 0;
	float:left;
	overflow:hidden;
}

.pagination li.current a {
	background-position:0 -12px;
}

/**** END OF SLIDER ***/
.paging
{
    position: absolute;
    bottom: 0px;
    left: 450px;
    height: 12px;
    z-index: 100;
    display: none;
}
.paging a
{
    display: block;
    width: 12px;
    height: 12px;
    background: url(../images/OuterImages/mark-up.gif);
    text-indent: -9999px;
    float: left;
    margin: 0px 7px 5px 0;
}
.paging a.active
{
    background: url(../images/OuterImages/mark-dwn.gif);
}



