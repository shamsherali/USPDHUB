$(function () {



    ///// Popup's
    $('[data-target]').click(function (e) { e.preventDefault(); var modalBox = $(this).attr('data-target'); $('#' + modalBox).fadeIn($(this).data()); $('body').addClass('modal-open'); });
    $('[data-dismiss]').click(function (e) { e.preventDefault(); $(".modal-overlay").fadeOut(); $('body').removeClass('modal-open'); });









});		
		

 		
		
		
	
 



 
 

